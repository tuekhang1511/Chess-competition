, self).__init__()\n        self.fc = nn.Sequential(\n            nn.Linear(input_size, 128),  # First layer\n            nn.ReLU(),\n            nn.Linear(128, 64),  # Second layer\n            nn.ReLU(),\n            nn.Linear(64, output_size)  # Output layer\n        )\n\n    def forward(self, x):\n        return self.fc(x)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T15:48:17.016004Z","iopub.execute_input":"2024-12-06T15:48:17.016356Z","iopub.status.idle":"2024-12-06T15:48:17.021891Z","shell.execute_reply.started":"2024-12-06T15:48:17.016322Z","shell.execute_reply":"2024-12-06T15:48:17.020980Z"}},"outputs":[],"execution_count":243},{"cell_type":"code","source":"class ReplayBuffer:\n    def __init__(self, max_size):\n        self.buffer = deque(maxlen=max_size)\n\n    def store(self, state, action, reward, next_state, done):\n        self.buffer.append((state, action, reward, next_state, done))\n\n    def sample(self, batch_size):\n        return random.sample(self.buffer, batch_size)\n\n    def size(self):\n        return len(self.buffer)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T15:48:17.313840Z","iopub.execute_input":"2024-12-06T15:48:17.314190Z","iopub.status.idle":"2024-12-06T15:48:17.319695Z","shell.execute_reply.started":"2024-12-06T15:48:17.314156Z","shell.execute_reply":"2024-12-06T15:48:17.318609Z"}},"outputs":[],"execution_count":244},{"cell_type":"code","source":"def train_batch(batch, policy_net, optimizer, loss_fn):\n    states, actions, rewards, next_states, dones = zip(*batch)\n\n    # Convert to tensors\n    states_tensor = torch.tensor(states, dtype=torch.float32).to(device)\n    next_states_tensor = torch.tensor(next_states, dtype=torch.float32).to(device)\n    rewards_tensor = torch.tensor(rewards, dtype=torch.float32).to(device)\n    dones_tensor = torch.tensor(dones, dtype=torch.float32).to(device)\n\n    # Predict Q-values\n    q_values = policy_net(states_tensor)\n    q_values_next = policy_net(next_states_tensor).detach()\n\n    # Update targets\n    targets = q_values.clone()\n    for i, action in enumerate(actions):\n        targets[i, action] = rewards_tensor[i] + (1 - dones_tensor[i]) * 0.99 * q_values_next[i].max()\n\n    # Compute loss and optimize\n    loss = loss_fn(q_values, targets)\n    optimizer.zero_grad()\n    loss.backward()\n    optimizer.step()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T15:48:17.740283Z","iopub.execute_input":"2024-12-06T15:48:17.741158Z","iopub.status.idle":"2024-12-06T15:48:17.747424Z","shell.execute_reply.started":"2024-12-06T15:48:17.741122Z","shell.execute_reply":"2024-12-06T15:48:17.746466Z"}},"outputs":[],"execution_count":245},{"cell_type":"code","source":"def train_dql(episodes=5000, batch_size=256, checkpoint_path=None):\n    buffer = ReplayBuffer(max_size=50000)\n    policy_net = DQNet(input_size=64, output_size=64).to(device)  # Ensure input_size is correct based on your features\n    optimizer = optim.Adam(policy_net.parameters(), lr=0.001)\n    scheduler = torch.optim.lr_scheduler.StepLR(optimizer, step_size=100, gamma=0.99)\n\n    loss_fn = nn.MSELoss()\n\n    # Load the saved model if a checkpoint path is provided\n    if checkpoint_path is not None:\n        checkpoint = torch.load(checkpoint_path, map_location=device, weights_only=True)\n        policy_net.load_state_dict(checkpoint['model_state_dict'])\n        optimizer.load_state_dict(checkpoint['optimizer_state_dict'])\n        start_episode = checkpoint['episode']  # You can start from where you left off\n        print(f\"Resumed training from episode {start_episode}\")\n    else:\n        start_episode = 1  # Start from the beginning if no checkpoint is provided\n\n    random_bot = RandomBot()\n\n    for episode in range(start_episode, episodes + 1):\n        print(\"Episodes: \" + str(episode))\n        game = Game(fen=\"r2q1rk1/pppb1ppp/2n2n2/8/2PPP3/2N1B3/PP3PPP/R2Q1RK1 b - - 0 1\")\n        state = feature_based_encoding(game)  # Ensure this function returns valid state\n        epsilon = max(0.05, 0.98 * (0.9999 ** episode))\n\n        while game.status == Game.NORMAL:\n            # Choose action (epsilon-greedy)\n            legal_moves = list(game.get_moves())\n            if random.random() < epsilon:  # Exploration\n                action_index = random.randint(0, len(legal_moves) - 1)\n                action = random.choice(legal_moves)\n            else:  # Exploitation\n                state_tensor = torch.tensor(state, dtype=torch.float32).unsqueeze(0).to(device)\n                q_values = policy_net(state_tensor)\n                action_index = q_values.argmax().item()\n                action_index = min(action_index, len(legal_moves) - 1)  # This ensures action_index is within valid range\n                action = legal_moves[action_index]\n\n            # Apply action\n            game.apply_move(action)\n            next_state = feature_based_encoding(game)  # Get next state after action is applied\n            reward = -0.0001 if game.status == Game.NORMAL else (1 if game.status == Game.CHECKMATE else 0.5)\n            done = game.status != Game.NORMAL\n\n            # Store experience\n            buffer.store(state, action_index, reward, next_state, done)\n            state = next_state  # Move to the next state\n\n            # Train the network if buffer is sufficiently filled\n            if buffer.size() >= batch_size:\n                batch = buffer.sample(batch_size)\n                train_batch(batch, policy_net, optimizer, loss_fn)\n\n            # Let the random bot take its turn\n            if game.status == Game.NORMAL:\n                bot_move = random_bot.choose_move(game, depth=4)  # Random bot chooses a move\n                if bot_move:\n                    game.apply_move(bot_move)  # Apply the bot's move\n                    next_state = feature_based_encoding(game)\n                    reward = -0.0001 if game.status == Game.NORMAL else (-1 if game.status == Game.CHECKMATE else 0.5)\n                    done = game.status != Game.NORMAL\n\n                    # Store bot's move experience\n                    buffer.store(state, action_index, reward, next_state, done)\n                    state = next_state\n\n        scheduler.step()\n\n        # Optionally log learning rate\n        if episode % 10 == 0:\n            current_lr = scheduler.get_last_lr()[0]\n            print(f\"Episode {episode}, Current Learning Rate: {current_lr}\")\n            print(f\"Episode {episode}, Current Exploration Rate: {epsilon}\")\n\n        \n\n        # Save the model checkpoint periodically\n        if episode % 10 == 0:  # Save every 10 episodes or as desired\n            torch.save({\n                'episode': episode,\n                'model_state_dict': policy_net.state_dict(),\n                'optimizer_state_dict': optimizer.state_dict(),\n            }, 'chess_model.pth')\n            torch.save({\n                'model_state_dict': torch.quantization.quantize_dynamic(\n                policy_net, {torch.nn.Linear}, dtype=torch.qint8\n            ).to('cpu').state_dict(),\n            }, 'quantized_model.pth')\n            \n    \n    print(\"Training complete!\")","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T17:08:25.674100Z","iopub.execute_input":"2024-12-06T17:08:25.675018Z","iopub.status.idle":"2024-12-06T17:08:25.689457Z","shell.execute_reply.started":"2024-12-06T17:08:25.674964Z","shell.execute_reply":"2024-12-06T17:08:25.688482Z"}},"outputs":[],"execution_count":356},{"cell_type":"code","source":"# train_dql()\ntrain_dql(episodes = 5000, checkpoint_path = 'chess_model.pth')","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T17:08:26.819053Z","iopub.execute_input":"2024-12-06T17:08:26.819773Z"}},"outputs":[{"name":"stdout","text":"Episodes: 1\nEpisodes: 2\nEpisodes: 3\nEpisodes: 4\nEpisodes: 5\nEpisodes: 6\nEpisodes: 7\nEpisodes: 8\nEpisodes: 9\nEpisodes: 10\nEpisode 10, Current Learning Rate: 0.001\nEpisode 10, Current Exploration Rate: 0.9790204408824207\nEpisodes: 11\nEpisodes: 12\n","output_type":"stream"}],"execution_count":null},{"cell_type":"code","source":"import random\nfrom Chessnut.game import InvalidMove  # Ensure you import the necessary exception\n\nclass RandomBot:\n    def __init__(self):\n        pass\n\n    def evaluate_board(self, game):\n        \"\"\"\n        Evaluates the board position based on material balance.\n        Positive values indicate an advantage for White, negative for Black.\n        \"\"\"\n        piece_values = {\n            \"P\": 1, \"N\": 3, \"B\": 3, \"R\": 5, \"Q\": 9, \"K\": 0,  # White pieces\n            \"p\": -1, \"n\": -3, \"b\": -3, \"r\": -5, \"q\": -9, \"k\": 0  # Black pieces\n        }\n        \n        # Get the board state\n        board = game.get_fen()\n        evaluation = 0\n        \n        # Evaluate material\n        for piece in board:\n            if piece in piece_values:\n                evaluation += piece_values[piece]\n        \n        return evaluation\n\n    def minimax(self, game, depth, alpha, beta, is_maximizing_player):\n        \"\"\"\n        Minimax algorithm with Alpha-Beta pruning.\n        \"\"\"\n        if depth == 0 or game.status != Game.NORMAL:\n            # If we've reached the max depth or the game is over, evaluate the board\n            return self.evaluate_board(game)\n        \n        legal_moves = list(game.get_moves())\n        \n        if is_maximizing_player:\n            max_eval = float('-inf')\n            if legal_moves:\n                for move in legal_moves:\n                    try:\n                        g = game  # Make a copy of the game state to avoid altering the original\n                        g.apply_move(move)\n                        eval = self.minimax(g, depth - 1, alpha, beta, False)\n                        max_eval = max(max_eval, eval)\n                        alpha = max(alpha, eval)\n                        if beta <= alpha:\n                            break  # Beta cut-off\n                    except InvalidMove:\n                        continue  # Skip illegal moves\n            return max_eval\n        else:\n            min_eval = float('inf')\n            if legal_moves:\n                for move in legal_moves:\n                    try:\n                        g = game  # Make a copy of the game state to avoid altering the original\n                        g.apply_move(move)\n                        eval = self.minimax(g, depth - 1, alpha, beta, True)\n                        min_eval = min(min_eval, eval)\n                        beta = min(beta, eval)\n                        if beta <= alpha:\n                            break  # Alpha cut-off\n                    except InvalidMove:\n                        continue  # Skip illegal moves\n            return min_eval\n\n    def choose_move(self, game, depth=3):\n        \"\"\"\n        Chooses the best move based on the Minimax algorithm with Alpha-Beta pruning.\n        \"\"\"\n        if game.state.player == 'w':\n            legal_moves = list(game.get_moves())\n    \n            if legal_moves:\n                return random.choice(legal_moves)  # Choose a random move\n            return None  # No legal moves left\n            \n            best_move = None\n            best_eval = float('-inf')  # For maximizer (White)\n        \n            if legal_moves:            \n                for move in legal_moves:\n                    # Check if the move is legal\n                    g = game  # Make a copy of the game state to avoid altering the original\n                    g.apply_move(move)\n                    move_eval = self.minimax(g, depth, float('-inf'), float('inf'), False)  # Minimize for opponent (Black)\n                            \n                    if move_eval > best_eval:\n                        best_eval = move_eval\n                        best_move = move\n                return best_move\n            else:\n                return None\n        else:\n            return None\n                        \n        \n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T16:51:36.625931Z","iopub.execute_input":"2024-12-06T16:51:36.626662Z","iopub.status.idle":"2024-12-06T16:51:36.638159Z","shell.execute_reply.started":"2024-12-06T16:51:36.626625Z","shell.execute_reply":"2024-12-06T16:51:36.637149Z"}},"outputs":[],"execution_count":346},{"cell_type":"code","source":"# import os\n\n# # Specify the file path\n# file_path = '/kaggle/working/chess_model.pth'\n\n# # Check if the file exists\n# if os.path.exists(file_path):\n#     # Delete the file\n#     os.remove(file_path)\n#     print(f\"{file_path} has been deleted.\")\n# else:\n#     print(f\"{file_path} does not exist.\")","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T14:53:01.949948Z","iopub.execute_input":"2024-12-06T14:53:01.950810Z","iopub.status.idle":"2024-12-06T14:53:01.956362Z","shell.execute_reply.started":"2024-12-06T14:53:01.950776Z","shell.execute_reply":"2024-12-06T14:53:01.955413Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"%%writefile main.py\nimport torch\nfrom Chessnut import Game\nimport random\nfrom model import DQNet  # Import the trained model class\n# from encoding import feature_based_encoding  # Ensure you have the feature encoder function\n\n# Load the trained and quantized model\ndevice = torch.device(\"cpu\")  # Quantized models are CPU-specific\npolicy_net = DQNet(input_size=64, output_size=64)  # Ensure this matches the unquantized model definition\ncheckpoint = torch.load(\"quantized_model.pth\", map_location=device, weights_only=True)\npolicy_net.load_state_dict(checkpoint['model_state_dict'], strict=False)\npolicy_net.eval()\n\ndef piece_position(fen):\n    \"\"\"\n    Convert the FEN string into a 2D board representation.\n    \n    Args:\n        fen: A string representing the FEN notation of the board state.\n    \n    Returns:\n        A 2D list representing the board with pieces.\n    \"\"\"\n    # Extract the piece positions (first part of FEN)\n    piece_positions = fen.split()[0]\n    \n    # Parse piece positions into a 2D array\n    board = []\n    for rank in piece_positions.split('/'):\n        row = []\n        for char in rank:\n            if char.isdigit():\n                row.extend([' '] * int(char))  # Empty squares\n            else:\n                row.append(char)  # Piece\n        board.append(row)\n    return board\n\ndef feature_based_encoding(game):\n    \"\"\"\n    Extract high-level features for feature-based encoding.\n    \n    Args:\n        game: A Chessnut Game object representing the current game state.\n    \n    Returns:\n        A list of features [material_balance, white_king_safety, black_king_safety, white_mobility, black_mobility, center_control].\n    \"\"\"\n    # Define the mapping for piece encoding\n    piece_map = {\n        \"P\": 1, \"N\": 2, \"B\": 3, \"R\": 4, \"Q\": 5, \"K\": 6,  # White pieces\n        \"p\": -1, \"n\": -2, \"b\": -3, \"r\": -4, \"q\": -5, \"k\": -6,  # Black pieces\n        \" \": 0  # Empty square\n    }\n    \n    # Get the board state\n    board = piece_position(game.get_fen())\n    \n    # Encode the board using list comprehension and mapping\n    encoded_board = [\n        piece_map.get(piece, 0) for row in board for piece in row\n    ]\n    \n    return encoded_board\n\ndef chess_bot(obs):\n    \"\"\"\n    Chess bot that predicts the best move using a trained neural network.\n    \"\"\"\n    game = Game(obs.board)\n    moves = list(game.get_moves())\n    \n    if not moves:\n        return None  # No legal moves available (e.g., checkmate or stalemate)\n    \n    # Get the board representation using feature-based encoding\n    state = feature_based_encoding(game)\n    state_tensor = torch.tensor(state, dtype=torch.float32).unsqueeze(0).to(device)\n    \n    # Predict Q-values for all legal moves\n    with torch.no_grad():  # Avoid gradients during inference\n        q_values = policy_net(state_tensor).numpy().flatten()\n    \n    # Map legal moves to their corresponding Q-values\n    move_q_values = {move: q_values[i] for i, move in enumerate(moves) if i < len(q_values)}\n    \n    # Select the move with the highest Q-value\n    best_move = max(move_q_values, key=move_q_values.get)\n    return best_move","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T16:45:20.500208Z","iopub.execute_input":"2024-12-06T16:45:20.500582Z","iopub.status.idle":"2024-12-06T16:45:20.507684Z","shell.execute_reply.started":"2024-12-06T16:45:20.500549Z","shell.execute_reply":"2024-12-06T16:45:20.506767Z"}},"outputs":[{"name":"stdout","text":"Overwriting main.py\n","output_type":"stream"}],"execution_count":340},{"cell_type":"code","source":"from kaggle_environments import make\nenv = make(\"chess\", debug=True)\n\nresult = env.run([\"main.py\", \"random\"])\nprint(\"Agent exit status/reward/time left: \")\n# look at the generated replay.json and print out the agent info\nfor agent in result[-1]:\n    print(\"\\t\", agent.status, \"/\", agent.reward, \"/\", agent.observation.remainingOverageTime)\nprint(\"\\n\")\n# render the game\nenv.render(mode=\"ipython\", width=500, height=500) ","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-06T17:02:24.662166Z","iopub.execute_input":"2024-12-06T17:02:24.662994Z","iopub.status.idle":"2024-12-06T17:02:27.982223Z","shell.execute_reply.started":"2024-12-06T17:02:24.662956Z","shell.execute_reply":"2024-12-06T17:02:27.981347Z"}},"outputs":[{"name":"stdout","text":"Agent exit status/reward/time left: \n\t DONE / 0.5 / 10\n\t DONE / 0.5 / 10\n\n\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"<IPython.core.display.HTML object>","text/html":"<iframe srcdoc=\"<!--\n  Copyright 2020 Kaggle Inc\n\n  Licensed under 